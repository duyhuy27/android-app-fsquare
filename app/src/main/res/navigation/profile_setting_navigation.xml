<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_setting_navigation"
    app:startDestination="@id/editProfileFragment">


    <fragment
        android:id="@+id/editProfileFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/addressFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.AddressFragment"
        android:label="fragment_address"
        tools:layout="@layout/fragment_address" >
        <action
            android:id="@+id/action_addressFragment_to_newAddressFragment"
            app:destination="@id/newAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/newAddressFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.NewAddressFragment"
        android:label="fragment_new_address"
        tools:layout="@layout/fragment_new_address" >
        <action
            android:id="@+id/action_newAddressFragment_to_addressFragment"
            app:destination="@id/addressFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/paymentFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.PaymentFragment"
        android:label="fragment_payment"
        tools:layout="@layout/fragment_payment" />
    <fragment
        android:id="@+id/securityFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.SecurityFragment"
        android:label="fragment_security"
        tools:layout="@layout/fragment_security" />
    <fragment
        android:id="@+id/languageFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.LanguageFragment"
        android:label="fragment_language"
        tools:layout="@layout/fragment_language" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="vn.md18.fsquareapplication.features.profileandsetting.ui.fragment.PrivacyPolicyFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" />
</navigation>